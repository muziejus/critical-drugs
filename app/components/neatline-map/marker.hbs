  {{#if (or
    (and
      (lte @point.startYear this.neatlineFilter.afterYear)
      (not @point.endYear)
    )
    (and
      (lte @point.startYear this.neatlineFilter.afterYear)
      (gte @point.endYear this.neatlineFilter.beforeYear)
    )
      )}}
    <@layers.circle-marker
      @lat={{this.latitude}}
      @lng={{this.longitude}}
      @title={{get @point.elementTexts "DCtitle"}}
      @className={{concat "timeline-marker item-id-" @point.id}}
      @onClick={{this.handleMarkerClick}}
      as |marker|>
        <marker.tooltip>
          {{get @point.elementTexts "DCtitle"}}
        </marker.tooltip>
    </@layers.circle-marker>
  {{/if}}
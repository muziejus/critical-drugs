  {{#if (or
    (and
      (lte this.startYear this.neatlineFilter.afterYear)
      (not this.endYear)
    )
    (and
      (lte this.startYear this.neatlineFilter.afterYear)
      (gte this.endYear this.neatlineFilter.beforeYear)
    )
      )}}
    <@layers.circle-marker
      @lat={{this.latitude}}
      @lng={{this.longitude}}
      @title={{@point.title}}
      @className={{concat "timeline-marker item-id-" @point.id}}
      @onClick={{this.handleMarkerClick}}
      as |marker|>
        <marker.tooltip>
          {{if @point.title @point.title @point.itemTitle}}
        </marker.tooltip>
    </@layers.circle-marker>
  {{/if}}
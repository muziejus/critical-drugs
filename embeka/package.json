{
  "name": "embeka",
  "version": "0.0.0",
  "scripts": {
    "build": "FORCE_COLOR=1 npm-run-all --aggregate-output --parallel \"build:*\"",
    "build:js": "rollup -c ./rollup.config.mjs",
    "prepublishOnly": "pnpm run build",
    "lint": "FORCE_COLOR=1 npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "FORCE_COLOR=1 npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint . --config ./config/.template-lintrc.js",
    "lint:hbs:fix": "pnpm run lint:hbs --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup -c ./rollup.config.mjs --watch"
  },
  "keywords": [
    "ember-addon"
  ],
  "description": "An addon for creating ambitious Omeka sites with Ember",
  "repository": "https://github.com/muziejus/embeka",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/plugin-syntax-decorators": "7.19.0",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/preset-typescript": "^7.17.12",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/template": "^0.8.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/ember-data": "^4.4.4",
    "@types/ember-data__adapter": "^4.0.0",
    "@types/ember-data__model": "^4.0.0",
    "@types/ember-data__serializer": "^4.0.0",
    "@types/ember-data__store": "^4.0.1",
    "@types/ember__application": "^4.0.0",
    "@types/ember__component": "^4.0.8",
    "@types/ember__debug": "^4.0.0",
    "@types/ember__destroyable": "^4.0.0",
    "@types/ember__helper": "^4.0.0",
    "@types/ember__object": "^4.0.2",
    "@types/ember__owner": "^4.0.0",
    "@types/ember__runloop": "^4.0.1",
    "@types/ember__test-helpers": "^2.0.0",
    "rollup": "3.6.0",
    "rollup-plugin-ts": "^3.0.0",
    "typescript": "~4.9.0"
  },
  "peerDependencies": {
    "ember-data": "4.7.2"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {}
  },
  "packageManager": "pnpm@7.18.1",
  "volta": {
    "extends": "../package.json"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./addon-main.js": "./addon-main.js"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4"
  }
}
